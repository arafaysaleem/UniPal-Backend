{
  "name": "nodejs-starter-template",
  "version": "1.0.0",
  "description": "[![BadgeNodeJS](https://img.shields.io/badge/MADE%20WITH-NODEJS-brightgreen?style=for-the-badge&logo=Node.js)](https://shields.io/) [![BadgeExpress](https://img.shields.io/badge/USES-EXPRESS-red?style=for-the-badge)](https://shields.io/) [![BadgeMySQL](https://img.shields.io/badge/USES-MYSQL-4479A1?style=for-the-badge&logo=MySQL)](https://shields.io/)",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "node -r dotenv-flow/config src/index.js -- --dotenv-flow-path=envs",
    "production": "cross-env NODE_ENV=production npm start",
    "dev": "cross-env NODE_ENV=dev DOTENV_FLOW_PATH=envs nodemon -r dotenv-flow/config",
    "test": "cross-env NODE_ENV=test DOTENV_FLOW_PATH=envs mocha -r dotenv-flow/config",
    "lint": "eslint src/",
    "fixlint": "eslint src/ --fix"
  },
  "mocha": {
    "exit": true,
    "spec": "./test/**/*.test.js",
    "timeout": "20000"
  },
  "keywords": [],
  "author": "Abdur Rafay Saleem",
  "license": "ISC",
  "resolutions": {
    "axios": "^0.21.1"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "deep-email-validator": "^0.1.18",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "otp-generator": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "decache": "^4.6.0",
    "eslint": "^7.25.0",
    "eslint-config-strongloop": "^2.1.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "sinon": "^11.1.2",
    "supertest": "^6.1.6"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arafaysaleem/nodejs_starter_template"
  }
}
